  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lzxb/vue-cnode/issues"
  },
  "homepage": "https://github.com/lzxb/vue-cnode#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
